 for both APY and the number of days being calculates as a proportion of the year\nvar apyAsDecimal=farmApy/100;var daysAsDecimalOfYear=numberOfDays/timesCompounded;//   Calculate the starting CAKE balance with a dollar balance of $1000.\nvar principal=1000/cakePrice;// This is a translation of the typical mathematical compounding APY formula. Details here: https://www.calculatorsoup.com/calculators/financial/compound-interest-calculator.php\nvar finalAmount=principal*Math.pow(1+apyAsDecimal/timesCompounded,timesCompounded*daysAsDecimalOfYear);// To get the cake earned, deduct the amount after compounding (finalAmount) from the starting CAKE balance (principal)\nvar interestEarned=finalAmount-principal;return roundToTwoDp(interestEarned);};var apyModalRoi=function apyModalRoi(_ref2){var amountEarned=_ref2.amountEarned,amountInvested=_ref2.amountInvested;var percentage=amountEarned/amountInvested*100;return percentage.toFixed(2);};\n// CONCATENATED MODULE: ./src/views/Farms/components/FarmCard/ApyCalculatorModal.tsx\nvar ApyCalculatorModal_templateObject,ApyCalculatorModal_templateObject2,ApyCalculatorModal_templateObject3;var Grid=styled_components_browser_esm[\"e\" /* default */].div(ApyCalculatorModal_templateObject||(ApyCalculatorModal_templateObject=Object(taggedTemplateLiteral[\"a\" /* default */])([\"\\n  display: grid;\\n  grid-template-columns: repeat(3, 1fr);\\n  grid-template-rows: repeat(4, auto);\\n  margin-bottom: 24px;\\n\"])));var GridItem=styled_components_browser_esm[\"e\" /* default */].div(ApyCalculatorModal_templateObject2||(ApyCalculatorModal_templateObject2=Object(taggedTemplateLiteral[\"a\" /* default */])([\"\\n  margin-bottom: '10px';\\n\"])));var Description=Object(styled_components_browser_esm[\"e\" /* default */])(uikit[\"Text\"])(ApyCalculatorModal_templateObject3||(ApyCalculatorModal_templateObject3=Object(taggedTemplateLiteral[\"a\" /* default */])([\"\\n  max-width: 320px;\\n  margin-bottom: 28px;\\n\"])));var ApyCalculatorModal_ApyCa